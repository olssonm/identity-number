{
    "name": "olssonm/identity-number",
    "description": "Laravel validator for Swedish personal identity numbers / social security numbers (personnummer)",
    "keywords": [
        "Laravel",
        "package",
        "olssonm",
        "swedish",
        "personal identity number",
        "social security number",
        "personnummer"
    ],
    "homepage": "https://github.com/olssonm/identity-number",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Olsson",
            "email": "contact@marcusolsson.me",
            "homepage": "https://marcusolsson.me"
        }
    ],
    "require": {
        "php" : "^7.1",
        "illuminate/support": "^5.8 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0 || ^8.0",
        "orchestra/testbench": ">=3.8.0"
    },
    "autoload": {
        "psr-4": {
            "Olssonm\\IdentityNumber\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olssonm\\IdentityNumber\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        },
        "laravel": {
            "providers": [
                "Olssonm\\IdentityNumber\\IdentityNumberServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
